{
  "private": true,
  "version": "0.1.0",
  "license": "MIT",
  "workspaces": [
    "contracts",
    "app"
  ],
  "scripts": {
    "compile": "npm run compile --workspace=my-contracts",
    "deploy": "npm run deploy --workspace=my-contracts",
    "build:contracts": "npm run build --workspace=my-contracts",
    "test": "npm run test --workspace=my-contracts",
    "dev": "npm run dev --workspace=my-dapp",
    "start": "npm run start --workspace=my-dapp",
    "build:app": "npm run build --workspace=my-dapp",
    "setup": "yarn compile && yarn deploy && yarn build:contracts",
    "devnet:start": "docker compose -f alephium-stack/devnet/docker-compose.yml up -d",
    "devnet:stop": "docker compose -f alephium-stack/devnet/docker-compose.yml down",
    "go": "yarn devnet:start && yarn compile && yarn deploy && yarn build:contracts && yarn dev",
    "go:testnet": "yarn compile && PRIVATE_KEYS=$PRIVATE_KEYS npx --yes @alephium/cli deploy --network testnet -w contracts && yarn build:contracts && NEXT_PUBLIC_NETWORK=testnet yarn dev",
    "dev:testnet": "NEXT_PUBLIC_NETWORK=testnet yarn dev"
  },
  "packageManager": "yarn@1.22.22+sha1.ac34549e6aa8e7ead463a7407e1c7390f61a6610"
}
